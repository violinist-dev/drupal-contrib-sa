{
  "name": "violinist-dev/drupal-contrib-sa",
  "description": "Drupal contrib SA",
  "type": "library",
  "require-dev": {
    "sensiolabs/security-advisories": "dev-master",
    "composer/composer": "^1.8"
  },
  "license": "MIT",
  "require": {
    "guzzlehttp/guzzle": "^6.3",
    "symfony/dom-crawler": "^4.2",
    "symfony/css-selector": "^4.2",
    "symfony/yaml": "^4.2",
    "symfony/cache": "^4.2",
    "symfony/console": "^4.2",
    "symfony/dependency-injection": "^4.2",
    "ext-json": "*",
    "vierbergenlars/php-semver": "^3.0",
    "psr/simple-cache": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Violinist\\DrupalContribSA\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Violinist\\DrupalContribSA\\": "tests"
    }
  },
  "scripts": {
    "test": "@composer lint && phpunit && phpstan analyse src test",
    "lint": "phpcs -p -n"
  },
  "extra": {
    "violinist": {
      "bundled_packages": {
        "symfony/console": [
          "symfony/*"
        ]
      },
      "blocklist": [
        "symfony/dom-crawler",
        "symfony/css-selector",
        "symfony/yaml",
        "symfony/cache",
        "symfony/dependency-injection"
      ]
    }
  }
}
